{
  "name": "Flutter Development",
  "build": {
    "dockerfile": ".gitpod.Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dart-code.flutter"
      ]
    }
  },
  "postCreateCommand": "dart --disable-analytics && flutter pub get && flutter create --platforms linux .",
  "portsAttributes": {
    "5900": {
      "label": "VNC",
      "onAutoForward": "ignore"
    },
    "6080": {
      "label": "Flutter Preview",
      "onAutoForward": "openPreview"
    },
    "8080": {
      "onAutoForward": "ignore"
    },
    "5037": {
      "onAutoForward": "ignore"
    },
    "9100-9101": {
      "onAutoForward": "ignore"
    },
    "40000-50000": {
      "onAutoForward": "ignore"
    }
  },
  "forwardPorts": [
    5900,
    6080,
    8080,
    5037,
    "9100-9101",
    "40000-50000"
  ],
  "remoteUser": "root"
}
